{
  "name": "planner",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.7.17",
    "apollo-link": "^1.2.14",
    "apollo-link-rest": "^0.9.0",
    "concurrently": "^8.2.0",
    "express": "^4.18.2",
    "graphql": "^15.3.0",
    "graphql-anywhere": "^4.2.8",
    "http-proxy-middleware": "^2.0.6",
    "javascript-time-ago": "^2.5.9",
    "node-fetch": "latest",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "react-time-ago": "^7.2.1",
    "relative-time-format": "^1.1.6",
    "remark-gfm": "^3.0.1"
  },
  "scripts": {
    "start": "tsc && npm run development",
    "development": "NODE_ENV=development concurrently --kill-others \"npm run client\" \"npm run server\"",
    "production": "npm run build && NODE_ENV=production npm run server",
    "server": "node server/server.js",
    "client": "react-scripts --openssl-legacy-provider start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "bash scripts/deploy.sh",
    "docker": "docker-compose -f docker-compose.prod.yml up -d --build"
  },
  "engines": {
    "node": "18.x"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ],
          "alias": {
            "@components": "./components",
            "@data": "./data"
          }
        }
      ]
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "babel-plugin-module-resolver": "^5.0.0",
    "@babel/plugin-proposal-private-property-in-object": "^7",
    "customize-cra": "^1.0.0",
    "react-scripts": "^5.0.1",
    "react-app-rewired": "^2.2.1",
    "react-error-overlay": "6.0.11",
    "tslint": "^6.1.3",
    "typescript": "^4.3.2"
  }
}
